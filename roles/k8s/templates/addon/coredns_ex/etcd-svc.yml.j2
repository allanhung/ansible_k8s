apiVersion: v1
kind: Service
metadata:
  labels:
    k8s-app: coredns-exetcd
  name: coredns-exetcd
  namespace: {{ pillar.k8s_addon.coredns_ex.namespace }}
spec:
  ports:
  - name: etcd-http
    port: 2379
  - name: etcd-peer
    port: 2380
  selector:
    k8s-app: coredns-exetcd
  type: ClusterIP
